<div class="row">
  <div class="small-12 columns">
    <h2>Examen Diagnostico - <%= @diagnostic_exam.subject %></h2>
  </div>
</div>
<div class="row">
  <div class="small-12 columns">
    <% @questions.each_with_index do |question, i| %>
      <div class="row">
        <div class="small-12 columns">
          <strong>Pregunta <%= i+1 %>:</strong>
          <%= markdown question.question %>
        </div>
      </div>
      <div class="row">
        <div class="small-12 columns">
          <% question.correct_answer.each do |correct_answers| %>
            Respuesta correcta: <%= correct_answers.answer %>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>

<div class="row">
  <div class="small-12 columns">
    <%= link_to 'Regresar', admin_dashboard_path(anchor: 'examenes_diagnositcos'), class: 'button tiny radius' %>
  </div>
</div>
